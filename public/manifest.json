{
  "manifest_version": 3,
  "name": "KindleRead",
  "description": "Allows you to upload and read books + many other options",
  "version": "1.0",
  "action": {}
 ,
  "permissions": [
    "tabs",
    "scripting",
    "webRequest"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["https://cdnjs.cloudflare.com/ajax/libs/pdf.js/3.11.174/pdf.worker.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
