{
  "manifest_version": 3,
  
  "name": "My Bookshelf",
  "description": "Allows you to upload and read books + many other options",
  "version": "1.0",
    "icons":   {
      "16": "openBook.png",
      "48": "openBook.png",
      "128": "openBook.png"
    }
  ,
  "action": {
    "default_icon":   {
      "16": "openBook.png",
      "48": "openBook.png",
      "128": "openBook.png"
    }
  },
  "permissions": [
    "tabs",
    "scripting",
    "webRequest"
  ],
 
  "background": {
    "service_worker": "background.js"
  }
 
  
}
